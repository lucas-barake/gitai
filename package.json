{
  "name": "gitai",
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@9.10.0",
  "license": "MIT",
  "description": "AI-powered git assistant",
  "bin": {
    "gitai": "src/Main.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lucas-barake/gitai"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "check": "tsc --noEmit",
    "lint": "eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\"",
    "lint-fix": "pnpm lint --fix",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "gitai": "bun run src/Main.ts",
    "format": "prettier --write \"**/*.{ts,mjs,json,md}\"",
    "build": "bun build src/Main.ts --compile --outfile gitai",
    "release": "bun scripts/release.ts"
  },
  "dependencies": {
    "@effect/ai": "^0.32.1",
    "@effect/ai-anthropic": "^0.22.0",
    "@effect/ai-google": "^0.11.0",
    "@effect/ai-openai": "^0.36.0",
    "@effect/cli": "^0.72.1",
    "@effect/cluster": "^0.55.0",
    "@effect/experimental": "^0.57.11",
    "@effect/platform": "^0.93.6",
    "@effect/platform-bun": "^0.86.0",
    "@effect/rpc": "^0.72.2",
    "effect": "^3.19.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-export-namespace-from": "^7.24.7",
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@effect/build-utils": "0.8.9",
    "@effect/eslint-plugin": "0.3.2",
    "@effect/language-service": "0.27.2",
    "@effect/vitest": "latest",
    "@eslint/compat": "1.1.1",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.10.0",
    "@types/node": "^22.5.2",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "dotenv-cli": "8.0.0",
    "eslint": "^9.10.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-codegen": "^0.28.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "tsx": "^4.17.0",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  },
  "pnpm": {
    "patchedDependencies": {
      "babel-plugin-annotate-pure-calls@0.4.0": "patches/babel-plugin-annotate-pure-calls@0.4.0.patch"
    }
  }
}
